/*!40000 ALTER TABLE `actas` ENABLE KEYS */;
UNLOCK TABLES;

--
-- Table structure for table `acuerdos`
--

DROP TABLE IF EXISTS `acuerdos`;
CREATE TABLE `acuerdos` (
  `acu_id` int(11) NOT NULL AUTO_INCREMENT,
  `acu_actid` int(6) NOT NULL,
  `acu_num` int(3) NOT NULL,
  `acu_tex0` text NOT NULL,
  `acu_conf` date NOT NULL,
  `acu_desc` varchar(210) DEFAULT NULL,
  `act_line` text NOT NULL,
  `act_rubr` text NOT NULL,
  PRIMARY KEY (`acu_id`),
  KEY `fk1_actas` (`acu_actid`)
) ENGINE=MyISAM AUTO_INCREMENT=1165 DEFAULT CHARSET=utf8;

--
-- Dumping data for table `acuerdos`
--

LOCK TABLES `acuerdos` WRITE;
/*!40000 ALTER TABLE `acuerdos` DISABLE KEYS */;
