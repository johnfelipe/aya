--
-- Table structure for table `relacion_actas`
--

DROP TABLE IF EXISTS `relacion_actas`;
CREATE TABLE `relacion_actas` (
  `relacion_actaid` int(6) NOT NULL,
  `relacion_nominaid` int(4) NOT NULL,
  `relacion_funcionario` int(4) NOT NULL,
  `relacion_profe` int(1) NOT NULL,
  KEY `fk6_actas` (`relacion_actaid`),
  KEY `fk4_per_consejo` (`relacion_nominaid`),
  KEY `fk4_nomina` (`relacion_funcionario`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

--
-- Dumping data for table `relacion_actas`
