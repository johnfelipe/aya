  `rep_id` int(11) NOT NULL AUTO_INCREMENT,
  `rep_acu` int(11) NOT NULL,
  `rep_a` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `rep_b` varchar(100) COLLATE utf8_unicode_ci NOT NULL,
  `rep_c` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `rep_d` varchar(30) COLLATE utf8_unicode_ci NOT NULL,
  `rep_f` varchar(12) CHARACTER SET ucs2 COLLATE ucs2_unicode_ci NOT NULL,
